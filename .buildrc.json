{
    "srcDir": "src",

    "mainModuleFileName": "main.js",

    "mainModuleName": "RServer",

    "fileExtensions": [".js"],

    "exclude": [],

    "include": ["*"],

    "externalModules": ["fs", "path", "hash", "crypto", "http", "https"],

    "copyAssets": true,

    "sourcemap": false,

    "distConfig": {
        "disabled": true,
        "format": "iife",
